#include <iostream>

using namespace std;

int main()
{

	char harf;
	cout << "baslangic harfini giriniz :";
	cin >> harf;

	int uzunluk;
	cout << "uzunlugu giriniz :";
	cin >> uzunluk;
	uzunluk = uzunluk + harf;

	for (int i = harf; i < uzunluk; i++) {
		for (int j = harf; j <= i; ++j) {
			cout << (char)j;

		}
		for (int k = i - 1; k >= harf; --k)
			cout << (char)k;
		cout << endl;

	}
	return 0;
}
	
